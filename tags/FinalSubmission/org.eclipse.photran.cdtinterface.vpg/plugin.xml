<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <!-- VPG-based model builder -->
   <extension
         point="org.eclipse.photran.cdtinterface.modelbuilder">
      <modelbuilder
            class="org.eclipse.photran.internal.core.model.FortranModelBuilder"
            id="defaultModelBilder"
            name="%modelbuilder.name" />
   </extension>
   
   <extension
         point="org.eclipse.photran.cdtinterface.modelbuilder">
      <modelbuilder
            class="org.eclipse.photran.internal.core.model.ConcurrentFortranModelBuilder"
            id="concurrentModelBuilder"
            name="%concurrentmb.name" />
   </extension>

</plugin>
